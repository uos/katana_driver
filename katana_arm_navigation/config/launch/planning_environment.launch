<?xml version="1.0"?>
<launch> 
	<!-- send parameters for collision checking for Katana; this includes parameters for the self filter -->
	<rosparam command="load" ns="robot_description_collision" file="$(find katana_arm_navigation)/config/config/collision_checks.yaml" />

  <!-- send parameters for multidof -->
  <rosparam command="load" ns="robot_description_planning" file="$(find katana_arm_navigation)/config/config/multi_dof_joints.yaml" />

  <!-- send parameters needed for motion planning -->
  <rosparam command="load" ns="robot_description_planning" file="$(find katana_arm_navigation)/config/config/planning_groups.yaml" />
</launch>
